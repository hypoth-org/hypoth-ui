{
  "$schema": "../../schemas/dtcg-token.schema.json",
  "$description": "Dark mode token overrides",
  "color": {
    "background": {
      "default": { "$value": "{color.primitives.gray.950}", "$type": "color" },
      "subtle": { "$value": "{color.primitives.gray.900}", "$type": "color" },
      "muted": { "$value": "{color.primitives.gray.800}", "$type": "color" },
      "emphasis": { "$value": "{color.primitives.gray.50}", "$type": "color" }
    },
    "foreground": {
      "default": { "$value": "{color.primitives.gray.50}", "$type": "color" },
      "muted": { "$value": "{color.primitives.gray.400}", "$type": "color" },
      "subtle": { "$value": "{color.primitives.gray.500}", "$type": "color" },
      "inverted": { "$value": "{color.primitives.gray.900}", "$type": "color" }
    },
    "border": {
      "default": { "$value": "{color.primitives.gray.800}", "$type": "color" },
      "muted": { "$value": "{color.primitives.gray.900}", "$type": "color" },
      "emphasis": { "$value": "{color.primitives.gray.600}", "$type": "color" }
    },
    "primary": {
      "default": { "$value": "{color.primitives.blue.500}", "$type": "color" },
      "hover": { "$value": "{color.primitives.blue.400}", "$type": "color" },
      "active": { "$value": "{color.primitives.blue.300}", "$type": "color" },
      "subtle": { "$value": "{color.primitives.blue.950}", "$type": "color" }
    },
    "destructive": {
      "default": { "$value": "{color.primitives.red.500}", "$type": "color" },
      "hover": { "$value": "{color.primitives.red.400}", "$type": "color" },
      "active": { "$value": "{color.primitives.red.300}", "$type": "color" },
      "subtle": { "$value": "{color.primitives.red.950}", "$type": "color" }
    },
    "success": {
      "default": { "$value": "{color.primitives.green.500}", "$type": "color" },
      "hover": { "$value": "{color.primitives.green.400}", "$type": "color" },
      "subtle": { "$value": "{color.primitives.green.950}", "$type": "color" }
    },
    "warning": {
      "default": { "$value": "{color.primitives.amber.400}", "$type": "color" },
      "hover": { "$value": "{color.primitives.amber.300}", "$type": "color" },
      "subtle": { "$value": "{color.primitives.amber.950}", "$type": "color" }
    }
  }
}
