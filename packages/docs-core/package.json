{
  "name": "@ds/docs-core",
  "version": "0.0.0",
  "type": "module",
  "description": "Headless documentation engine with tenant filtering",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run",
    "validate:manifests": "tsx src/cli/validate-manifests.ts"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "tsx": "^4.7.0",
    "ajv": "^8.12.0",
    "gray-matter": "^4.0.3"
  }
}
