{
  "name": "@hypoth-ui/cli",
  "version": "0.0.1",
  "description": "CLI tool for adding hypoth-ui components to your project",
  "type": "module",
  "bin": {
    "hypoth-ui": "./dist/index.js"
  },
  "files": ["dist", "registry", "templates"],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:watch": "vitest",
    "sync:templates": "tsx scripts/sync-templates.ts"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.1.1",
    "execa": "^9.5.2",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["cli", "design-system", "components", "hypoth-ui"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypoth-ui/hypoth-ui.git",
    "directory": "packages/cli"
  }
}
